{
    "name": "masunov/laravel-dto-yml-generator",
    "description": "Laravel package for build DTOs and YML definitions for JMS Serializer",
    "type": "library",
    "require": {
        "gears/class-finder": "^1.0",
        "symfony/yaml": "^4.1"
    },
    "authors": [
        {
            "name": "Vladislav Masunov",
            "email": "vlad@masunov.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Masunov\\LaravelDtoYmlGenerator\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Masunov\\LaravelDtoYmlGenerator\\LaravelDtoYmlGeneratorServiceProvider"
            ]
        }
    }
}
